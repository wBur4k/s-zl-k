meme_dict = {
             "CRINGE": "Garip ya da utandırıcı bir şey" ,
             "LOL": "Komik bir şeye verilen cevap" ,
             "SHEESH": "BİR SEYI ONAYLAMAK" ,
             "AGGRO" : "AGRESIFLESMEK"
            }

word = input("merak ettiğin kelime").upper()
if word in meme_dict.keys():
    print(meme_dict[word])
else:
    print("kelimeni bulamıyorum")
